  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@9.5.1",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "html2canvas": "^1.4.1",
    "tailwind-scrollbar": "^3.0.0"
  }
}
